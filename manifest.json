{
	"name": "Ares",
	"description": "An in-browser Enyo IDE",
	"version": "0.0.1",
	"manifest_version": 2,
	"minimum_chrome_version": "23",
	"offline_enabled": false,
	"app": {
		"background": {
			"scripts": ["chrome-app.js"]
		}
	},
	"icons": {
		"16": "ares/ares-16.png",
		"128": "ares/ares-128.png"
	},
	"permissions": [
		"http://enyojs.com/",
		"storage",
		"unlimitedStorage",
		"clipboardRead",
		"clipboardWrite",
		{"fileSystem": ["write"]}
	],
	"sandbox": {
	   	"pages": [
	   		"ares/index.html"
	   	]
	}
}