{
	"domain": null,
	"services": [
		{
			"active":true,
			"id":"home",
			"icon":"home-32x32",
			"name":"Home Directory",
			"type": "filesystem",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/fsLocal.js", "--pathname", "/files", "--port", "0", "--root", "@HOME@"
			],
			"useJsonp":false,
			"verbose": false,
			"respawn": false
		},
		{
			"active":true,
			"id":"dropbox",
			"icon":"dropbox.com-32x32",
			"name":"Dropbox",
			"type": "filesystem",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/fsDropbox.js", "-P", "/files", "-p", "0"
			],
			"auth": {
				"type": "dropbox",
				"appKey": "",
				"appSecret": ""
			},
			"Xproxy":{
				"http":{
					"tunnel":"OverHttp",
					"host":"web-proxy.corp.hp.com",
					"port":8080
				},
				"https":{
					"tunnel":"OverHttp",
					"host":"web-proxy.corp.hp.com",
					"port":8080
				}
			},
			"useJsonp":false,
			"verbose": false
		},
		{
			"active":true,
			"id":"phonegap",
			"icon":"build.phonegap.com-32x32",
			"name":"PhoneGap Build",
			"type": "build",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/bdPhoneGap.js", "-P", "/phonegap", "-p", "0"
			],
			"useJsonp":false,
			"verbose": false,
			"auth": {
				"type": "phonegap"
			},
			"respawn": false
		},
		{
			"active": true,
			"id":"prj-toolkit",
			"name":"Project toolkit",
			"type": "other",
			"provider": "hermes",
			"command":"@NODE@", "params":[
				"hermes/prjToolkit.js", "-P", "/prj-toolkit", "-p", "0"
			],
			"project-template-repositories": [{
				"id": "bootplate",
				"url": "http://enyojs.com/archive/ares-project-templates.json"
			}],
			"useJsonp":false,
			"verbose": true,
			"respawn": false
		}
	]
}
